{
  "Driver:Demo": "yay",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext"
    ],
    "Using":[
       "Serilog.Sinks.Console"
    ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff} [{Level}] {SourceContext} {Message:l} {Properties:l}{NewLine}{Exception}"
        }
      }
    ],
    "Properties": {
      "Application": "EquipmentScreen"
    }
  }
}
